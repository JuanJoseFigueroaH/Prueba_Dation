{
  "name": "BackendApi",
  "version": "1.0.0",
  "description": "Api Backend Dation",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/index",
    "generate-types": "prisma generate",
    "seeds-order": "ts-node ./src/seeders/_listOrder.seeder.ts",
    "seeds": "ts-node ./src/seeders/_main.seeder.ts",
    "seed": "ts-node ./src/seeders/_individual.seeder.ts",
    "clean-db": "prisma migrate reset",
    "sync-db-tables": "prisma db push",
    "sync-db-migrate": "prisma migrate dev",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit"
  },
  "author": "Juan Jose Figueroa Hurtatis",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/inquirer": "^7.3.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.170",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.1",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "6.0.0",
    "prisma": "^2.30.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@prisma/client": "^2.24.1",
    "@sendgrid/mail": "^7.4.4",
    "@types/express-fileupload": "^1.1.6",
    "async": "^3.2.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.11.1",
    "inquirer": "^8.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "prisma-schema-import": "^1.2.5",
    "socket.io": "^4.1.3",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2"
  }
}
